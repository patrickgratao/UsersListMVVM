{
  "name": "@patrick_gratao/users-mvvm",
  "description": "Lista de usuÃ¡rios com MVVM",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
